<h1>New Home Listing</h1>

<form action="/homes" method="post">
  <div class="field">
    <label for="home_address">Address:</label>
    <input type="text" id="home_address" name="home[address]">
  </div>

  <div class="field">
    <label for="home_description">Description:</label>
    <textarea id="home_description" name="home[description]"></textarea>
  </div>

  <div class="field">
    <label for="home_year_built">Year Built:</label>
    <input type="number" id="home_year_built" name="home[year_built]">
  </div>

  <div class="field">
    <label for="home_bathrooms">Number of Bathrooms:</label>
    <input type="number" id="home_bathrooms" name="home[bathrooms]">
  </div>

  <div class="field">
    <label for="home_square_feet">Square Footage:</label>
    <input type="number" id="home_square_feet" name="home[square_feet]">
  </div>

  <!-- Add more fields here for other attributes -->

  <div class="actions">
    <input type="submit" value="Create Home Listing">
  </div>
</form>
